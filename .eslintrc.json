{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": ["react-app", "prettier"],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "plugins": ["react", "prettier", "jsx-a11y"],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "printWidth": 80,
                "trailingComma": "es5",
                "semi": true,
                "jsxSingleQuote": false,
                "singleQuote": false,
                "useTabs": true
            }
        ]
    }
}